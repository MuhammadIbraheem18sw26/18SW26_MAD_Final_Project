<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shopkar0">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GroceryCity">
        <activity android:name="com.example.shopkar0.activites.PlaceOrderActivity"></activity>
        <activity android:name="com.example.shopkar0.activites.CategoryItemViewActivity" />
        <activity android:name="com.example.shopkar0.activites.ItemsDetailActivity" />
        <activity android:name="com.example.shopkar0.activites.ViewAllActivity" />
        <activity android:name="com.example.shopkar0.activites.WelcomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.shopkar0.activites.SignInActivity" />
        <activity android:name="com.example.shopkar0.activites.SignUpActivity" />
        <activity
            android:name="com.example.shopkar0.activites.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.GroceryCity.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>